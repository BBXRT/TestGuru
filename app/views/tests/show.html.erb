<h1>Вопросы:</h1>

<table>

  <% @questions.each do |question| %>
    <tr>
      <td><%= question.body %></td>

      <td><%= button_to "Удалить", question_path(question), method: :delete %></td>

    </tr>
  <% end %>

  <td><%= button_to "Назад к списку вопросов", tests_path, method: :get %></td>
</table>
